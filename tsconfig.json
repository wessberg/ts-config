{
	"compilerOptions": {
		"strict": true,
		"skipLibCheck": true,
		"alwaysStrict": true,
		"emitDecoratorMetadata": false,
		"importHelpers": true,
		"newLine": "LF",
		"sourceMap": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"moduleResolution": "nodenext",
		"module": "esnext",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"isolatedModules": true,
		"verbatimModuleSyntax": true,
		"resolveJsonModule": true,
		"target": "esnext",
		"lib": [
			"es5",
			"es2015",
			"es6",
			"es7",
			"es2016",
			"es2017",
			"es2018",
			"es2019",
			"es2020",
			"es2021",
			"es2022",
			"es2023",
			"esnext",
			"dom",
			"dom.iterable",
			"webworker",
			"webworker.importscripts",
			"scripthost"
		]
	}
}
